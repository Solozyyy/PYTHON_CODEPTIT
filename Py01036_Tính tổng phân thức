from math import *

for tc in range(int(input())):     
    n = int(input())
    s1, s2 = 0, 0
    if n%2 == 0:
        for i in range(2, n + 1, 2):
            s1 += float(1/i)
        print('{:.6f}'.format(s1))
    else:
        for i in range(1,n+1, 2):
            s2 += float(1/i)
        print('{:.6f}'.format(s2))
